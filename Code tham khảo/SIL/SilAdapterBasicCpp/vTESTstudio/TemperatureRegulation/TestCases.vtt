  <ttlib>
    <ts_definitions />
    <tc_definitions>
      <tc_definition>
        <name>SwitchHeatingState</name>
        <tcid>ea9cf062-2a58-4fd1-a487-a6ccd552205c</tcid>
        <parameters>
          <parameter_definition>
            <name>pSensorValue1</name>
            <type>int</type>
          </parameter_definition>
          <parameter_definition>
            <name>pSensorValue2</name>
            <type>int</type>
          </parameter_definition>
          <parameter_definition>
            <name>pSensorValue3</name>
            <type>int</type>
          </parameter_definition>
          <parameter_definition>
            <name>pExpectedIsHeating</name>
            <type>int</type>
          </parameter_definition>
          <parameter_definition>
            <name>pExpectedIsCooling</name>
            <type>int</type>
          </parameter_definition>
        </parameters>
        <set>
          <title>Set comfort temperature (no heating, no cooling)</title>
          <in>
            <assignment>
              <sink>
                <dbobject>VE_BEGIN_OF_OBJECT|1|RoomTemperatureControl::Sensor1.Temperature|END_OF_OBJECT_VE|</dbobject>
              </sink>
              <source>
                <const>20</const>
              </source>
            </assignment>
            <assignment>
              <sink>
                <dbobject>VE_BEGIN_OF_OBJECT|1|RoomTemperatureControl::Sensor2.Temperature|END_OF_OBJECT_VE|</dbobject>
              </sink>
              <source>
                <const>20</const>
              </source>
            </assignment>
            <assignment>
              <sink>
                <dbobject>VE_BEGIN_OF_OBJECT|1|RoomTemperatureControl::Sensor3.Temperature|END_OF_OBJECT_VE|</dbobject>
              </sink>
              <source>
                <const>20</const>
              </source>
            </assignment>
          </in>
        </set>
        <wait>
          <title>Wait for cyclic update of application</title>
          <time>
            <value>
              <dbobject>ChParScalar_BEGIN_OF_OBJECT|1|MaxReactionTime|END_OF_OBJECT_ChParScalar|</dbobject>
            </value>
            <unit>ms</unit>
          </time>
        </wait>
        <statecheck>
          <title>Check no heating, no cooling</title>
          <expected>
            <compare>
              <value>
                <dbobject>VE_BEGIN_OF_OBJECT|1|RoomTemperatureControl::Heating.CoolingState|END_OF_OBJECT_VE|</dbobject>
              </value>
              <eq>
                <const>0</const>
              </eq>
            </compare>
            <compare>
              <value>
                <dbobject>VE_BEGIN_OF_OBJECT|1|RoomTemperatureControl::Heating.HeatingState|END_OF_OBJECT_VE|</dbobject>
              </value>
              <eq>
                <const>0</const>
              </eq>
            </compare>
          </expected>
        </statecheck>
        <set>
          <title>Set new temperature values</title>
          <in>
            <assignment>
              <sink>
                <dbobject>VE_BEGIN_OF_OBJECT|1|RoomTemperatureControl::Sensor1.Temperature|END_OF_OBJECT_VE|</dbobject>
              </sink>
              <source>
                <variable>pSensorValue1</variable>
              </source>
            </assignment>
            <assignment>
              <sink>
                <dbobject>VE_BEGIN_OF_OBJECT|1|RoomTemperatureControl::Sensor2.Temperature|END_OF_OBJECT_VE|</dbobject>
              </sink>
              <source>
                <variable>pSensorValue2</variable>
              </source>
            </assignment>
            <assignment>
              <sink>
                <dbobject>VE_BEGIN_OF_OBJECT|1|RoomTemperatureControl::Sensor3.Temperature|END_OF_OBJECT_VE|</dbobject>
              </sink>
              <source>
                <variable>pSensorValue3</variable>
              </source>
            </assignment>
          </in>
        </set>
        <wait>
          <title>Wait for cyclic update of application</title>
          <time>
            <value>
              <dbobject>ChParScalar_BEGIN_OF_OBJECT|1|MaxReactionTime|END_OF_OBJECT_ChParScalar|</dbobject>
            </value>
            <unit>ms</unit>
          </time>
        </wait>
        <statecheck>
          <title>Check expected heating state</title>
          <expected>
            <compare>
              <value>
                <dbobject>VE_BEGIN_OF_OBJECT|1|RoomTemperatureControl::Heating.CoolingState|END_OF_OBJECT_VE|</dbobject>
              </value>
              <eq>
                <variable>pExpectedIsCooling</variable>
              </eq>
            </compare>
            <compare>
              <value>
                <dbobject>VE_BEGIN_OF_OBJECT|1|RoomTemperatureControl::Heating.HeatingState|END_OF_OBJECT_VE|</dbobject>
              </value>
              <eq>
                <variable>pExpectedIsHeating</variable>
              </eq>
            </compare>
          </expected>
        </statecheck>
      </tc_definition>
      <tc_definition>
        <name>ThresholdBehavior</name>
        <tcid>c4dfcb87-b4e1-4bf6-8008-f0675ba59992</tcid>
        <parameters>
          <parameter_definition>
            <name>pThreshold</name>
            <type>int</type>
          </parameter_definition>
          <parameter_definition>
            <name>pIsHeatingThreshold</name>
            <type>int</type>
          </parameter_definition>
        </parameters>
        <set>
          <title>Set threshold value</title>
          <in>
            <assignment>
              <sink>
                <dbobject>VE_BEGIN_OF_OBJECT|1|RoomTemperatureControl::Sensor1.Temperature|END_OF_OBJECT_VE|</dbobject>
              </sink>
              <source>
                <variable>pThreshold</variable>
              </source>
            </assignment>
            <assignment>
              <sink>
                <dbobject>VE_BEGIN_OF_OBJECT|1|RoomTemperatureControl::Sensor2.Temperature|END_OF_OBJECT_VE|</dbobject>
              </sink>
              <source>
                <variable>pThreshold</variable>
              </source>
            </assignment>
            <assignment>
              <sink>
                <dbobject>VE_BEGIN_OF_OBJECT|1|RoomTemperatureControl::Sensor3.Temperature|END_OF_OBJECT_VE|</dbobject>
              </sink>
              <source>
                <variable>pThreshold</variable>
              </source>
            </assignment>
          </in>
        </set>
        <wait>
          <title>Wait for cyclic update of application</title>
          <time>
            <value>
              <dbobject>ChParScalar_BEGIN_OF_OBJECT|1|MaxReactionTime|END_OF_OBJECT_ChParScalar|</dbobject>
            </value>
            <unit>ms</unit>
          </time>
        </wait>
        <statecheck>
          <title>Check no heating, no cooling</title>
          <expected>
            <compare>
              <value>
                <dbobject>VE_BEGIN_OF_OBJECT|1|RoomTemperatureControl::Heating.CoolingState|END_OF_OBJECT_VE|</dbobject>
              </value>
              <eq>
                <const>0</const>
              </eq>
            </compare>
            <compare>
              <value>
                <dbobject>VE_BEGIN_OF_OBJECT|1|RoomTemperatureControl::Heating.HeatingState|END_OF_OBJECT_VE|</dbobject>
              </value>
              <eq>
                <const>0</const>
              </eq>
            </compare>
          </expected>
        </statecheck>
        <if>
          <title>If threshold is for heating, check switch to heating</title>
          <compare>
            <value>
              <variable>pIsHeatingThreshold</variable>
            </value>
            <eq>
              <const>1</const>
            </eq>
          </compare>
          <set>
            <title>Decrease sensor value 1 by 1 C°</title>
            <in>
              <assignment>
                <sink>
                  <dbobject>VE_BEGIN_OF_OBJECT|1|RoomTemperatureControl::Sensor1.Temperature|END_OF_OBJECT_VE|</dbobject>
                </sink>
                <source>
                  <expression>
                    <const>pThreshold - 1</const>
                  </expression>
                </source>
              </assignment>
            </in>
          </set>
          <wait>
            <title>Wait for cyclic update of application</title>
            <time>
              <value>
                <dbobject>ChParScalar_BEGIN_OF_OBJECT|1|MaxReactionTime|END_OF_OBJECT_ChParScalar|</dbobject>
              </value>
              <unit>ms</unit>
            </time>
          </wait>
          <statecheck>
            <title>Check heating on</title>
            <expected>
              <compare>
                <value>
                  <dbobject>VE_BEGIN_OF_OBJECT|1|RoomTemperatureControl::Heating.CoolingState|END_OF_OBJECT_VE|</dbobject>
                </value>
                <eq>
                  <const>0</const>
                </eq>
              </compare>
              <compare>
                <value>
                  <dbobject>VE_BEGIN_OF_OBJECT|1|RoomTemperatureControl::Heating.HeatingState|END_OF_OBJECT_VE|</dbobject>
                </value>
                <eq>
                  <const>1</const>
                </eq>
              </compare>
            </expected>
          </statecheck>
        </if>
        <else>
          <title>Else (if threshold is for cooling), check switch to cooling</title>
          <set>
            <title>Increase sensor value 1 by 1 C°</title>
            <in>
              <assignment>
                <sink>
                  <dbobject>VE_BEGIN_OF_OBJECT|1|RoomTemperatureControl::Sensor1.Temperature|END_OF_OBJECT_VE|</dbobject>
                </sink>
                <source>
                  <expression>
                    <const>pThreshold + 1</const>
                  </expression>
                </source>
              </assignment>
            </in>
          </set>
          <wait>
            <title>Wait for cyclic update of application</title>
            <time>
              <value>
                <dbobject>ChParScalar_BEGIN_OF_OBJECT|1|MaxReactionTime|END_OF_OBJECT_ChParScalar|</dbobject>
              </value>
              <unit>ms</unit>
            </time>
          </wait>
          <statecheck>
            <title>Check cooling on</title>
            <expected>
              <compare>
                <value>
                  <dbobject>VE_BEGIN_OF_OBJECT|1|RoomTemperatureControl::Heating.CoolingState|END_OF_OBJECT_VE|</dbobject>
                </value>
                <eq>
                  <const>1</const>
                </eq>
              </compare>
              <compare>
                <value>
                  <dbobject>VE_BEGIN_OF_OBJECT|1|RoomTemperatureControl::Heating.HeatingState|END_OF_OBJECT_VE|</dbobject>
                </value>
                <eq>
                  <const>0</const>
                </eq>
              </compare>
            </expected>
          </statecheck>
        </else>
      </tc_definition>
    </tc_definitions>
